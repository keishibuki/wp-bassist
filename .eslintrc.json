{
	"env": {
		"es6": true,
		"browser": true
	},
	"ignorePatterns": ["**/*.config.js"],
	"globals": {},
	"extends": ["wordpress"],
	"rules": {
		"no-alert": "warn",
		"no-console": "warn",
		"no-unused-vars": "warn",
		"no-var": "warn",
		"eqeqeq": "warn"
	},
	"parserOptions": {
		"sourceType": "module"
	}
}
